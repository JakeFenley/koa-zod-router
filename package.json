{
  "name": "koa-zod-router",
  "version": "0.9.14",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format cjs,esm --dts",
    "build:turbo": "turbo run build",
    "example": "tsup examples/index.ts --watch",
    "test": "mocha",
    "test:coverage": "nyc --reporter=text mocha",
    "test:turbo": "turbo run test:coverage",
    "lint": "tsc && eslint \"src/**/*.ts\" --cache && prettier --check \"src/**/*.ts\"",
    "lint:turbo": "turbo run lint",
    "release": "pnpm build:turbo && npm publish"
  },
  "author": {
    "name": "Jake Fenley",
    "url": "https://github.com/jakefenley"
  },
  "license": "MIT",
  "dependencies": {
    "@koa/router": "^12.0.0",
    "formidable": "^2.1.1",
    "koa-bodyparser": "^4.3.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/formidable": "^2.0.5",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.11.18",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "koa": "^2.14.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.2",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "zod": ">=3.20.2 <4.x"
  },
  "keywords": [
    "nodejs",
    "api",
    "http",
    "middleware",
    "koa",
    "router",
    "json",
    "typescript",
    "schema",
    "server",
    "handler",
    "schema-validation",
    "endpoint",
    "zod",
    "validation"
  ]
